# OpenRouter Configuration (OPTIONAL)
# Get your API key from https://openrouter.ai/keys
# Leave empty to use local fallback content generation
OPENROUTER_API_KEY=

# AI Image Generation Providers (OPTIONAL)
# The system will try providers in order and fallback to Unsplash if none configured

# OpenAI DALL-E
# Get your API key from https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Stability AI 
# Get your API key from https://platform.stability.ai/account/keys
STABILITY_API_KEY=

# Replicate
# Get your API token from https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=

# Together AI
# Get your API key from https://api.together.xyz/settings/api-keys
TOGETHER_API_KEY=

# Optional: Use in client-side code (not recommended for production)
# NEXT_PUBLIC_OPENROUTER_API_KEY=

# Vercel deployment URL (automatically set by Vercel)
VERCEL_URL=
NEXT_PUBLIC_VERCEL_URL=

# Development settings
NODE_ENV=development
